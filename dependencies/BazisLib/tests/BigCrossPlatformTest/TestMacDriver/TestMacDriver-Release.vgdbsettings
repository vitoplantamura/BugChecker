<?xml version="1.0"?>
<VisualGDBProjectSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <BuildType>RemoteUnix</BuildType>
  <Settings xsi:type="RemoteBuildSettings">
    <BuildCommand>
      <Command>make</Command>
      <Arguments>CONFIG=RELEASE</Arguments>
      <WorkingDirectory>$(BuildDir)</WorkingDirectory>
      <Environment>
        <Records>
          <Record>
            <VariableName>LANG</VariableName>
            <Value>en_US</Value>
          </Record>
        </Records>
      </Environment>
    </BuildCommand>
    <CleanCommand>
      <Command>make</Command>
      <Arguments>CONFIG=RELEASE clean</Arguments>
      <WorkingDirectory>$(BuildDir)</WorkingDirectory>
      <Environment>
        <Records>
          <Record>
            <VariableName>LANG</VariableName>
            <Value>en_US</Value>
          </Record>
        </Records>
      </Environment>
    </CleanCommand>
    <LaunchGDBCommand>
      <Command>gdb</Command>
      <Arguments>--interpreter mi --args "$(TargetPath)"</Arguments>
      <WorkingDirectory>$(BuildDir)</WorkingDirectory>
      <Environment>
        <Records>
          <Record>
            <VariableName>LANG</VariableName>
            <Value>en_US</Value>
          </Record>
        </Records>
      </Environment>
    </LaunchGDBCommand>
    <DelayAfterGDBServerCommand>0</DelayAfterGDBServerCommand>
    <TerminateGDBServerOnExit>true</TerminateGDBServerOnExit>
    <SimulateCtrlCToBreak>false</SimulateCtrlCToBreak>
    <RemoteHost>
      <HostName>sysmacbook</HostName>
    </RemoteHost>
    <MainSourceTransferCommand>
      <RemoteHost>
        <HostName>sysmacbook</HostName>
      </RemoteHost>
      <LocalDirectory>$(ProjectDir)</LocalDirectory>
      <RemoteDirectory>/tmp/VisualGDB/TestMacDriver</RemoteDirectory>
      <FileMasks>
        <string>*.cpp</string>
        <string>*.h</string>
        <string>*.c</string>
        <string>*.mak</string>
        <string>Makefile</string>
      </FileMasks>
      <TransferNewFilesOnly>true</TransferNewFilesOnly>
      <IncludeSubdirectories>true</IncludeSubdirectories>
    </MainSourceTransferCommand>
  </Settings>
  <DefaultGDBSettings>
    <FilterSpuriousStoppedNotifications>false</FilterSpuriousStoppedNotifications>
    <ForceSingleThreadedMode>false</ForceSingleThreadedMode>
    <PendingBreakpointsSupported>true</PendingBreakpointsSupported>
    <DisableChildRanges>false</DisableChildRanges>
    <UseAppleExtensions>false</UseAppleExtensions>
    <CanAcceptCommandsWhileRunning>false</CanAcceptCommandsWhileRunning>
    <MakeLogFile>false</MakeLogFile>
    <IgnoreModuleEventsWhileStepping>true</IgnoreModuleEventsWhileStepping>
    <UseRelativePathsOnly>false</UseRelativePathsOnly>
    <ExitAction>None</ExitAction>
    <Features>
      <DisableAutoDetection>false</DisableAutoDetection>
      <UseFrameParameter>false</UseFrameParameter>
      <SimpleValuesFlagSupported>false</SimpleValuesFlagSupported>
      <ListLocalsSupported>false</ListLocalsSupported>
      <ByteLevelMemoryCommandsAvailable>false</ByteLevelMemoryCommandsAvailable>
      <ThreadInfoSupported>false</ThreadInfoSupported>
      <PendingBreakpointsSupported>false</PendingBreakpointsSupported>
    </Features>
    <DisableDisassembly>false</DisableDisassembly>
    <ExamineMemoryWithXCommand>false</ExamineMemoryWithXCommand>
  </DefaultGDBSettings>
  <GDBStartup>
    <SessionStartMode>UsingRun</SessionStartMode>
  </GDBStartup>
  <CustomSourceDirectories>
    <Directories />
    <MapCygdrive>false</MapCygdrive>
    <MapMinGW>false</MapMinGW>
    <ConvertSlashes>true</ConvertSlashes>
  </CustomSourceDirectories>
  <MainBuildDirectory>$(ProjectDir)</MainBuildDirectory>
  <PathOfMainBinary>$(ConfigurationName)/TestMacDriver</PathOfMainBinary>
  <ConfigurationName>Release</ConfigurationName>
</VisualGDBProjectSettings>